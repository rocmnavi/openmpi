    if test "$compile_mode" = "static"
then :
  if eval test \${pnet_$component_WRAPPER_EXTRA_LDFLAGS+y}
then :
  eval tmp_flags=\$pnet_$component_WRAPPER_EXTRA_LDFLAGS
else $as_nop
  eval tmp_flags=\$pnet_$component_LDFLAGS
fi


oac_var_scope_push ${LINENO} oac_list_prefix oac_list_append oac_list_arg oac_list_val

for oac_list_arg in $tmp_flags; do
    oac_list_append=1
    case ${oac_list_arg} in #(
  -I*|-L*|-l*) :
    for oac_list_val in ${pmix_mca_wrapper_extra_ldflags}; do
                 if test "x${oal_list_val}" = "x${oac_list_arg}"
then :
  oac_list_append=0
fi
             done ;; #(
  *) :
     ;;
esac
    if test ${oac_list_append} -eq 1
then :

if test -z "${pmix_mca_wrapper_extra_ldflags}"
then :
  pmix_mca_wrapper_extra_ldflags="$oac_list_arg"
else $as_nop
  pmix_mca_wrapper_extra_ldflags="${pmix_mca_wrapper_extra_ldflags} $oac_list_arg"
fi

fi
done

