        fi
    fi


else $as_nop
  pmix_cv_cc_fsanundefined=1


    if test -s conftest.err ; then
        $GREP -iq "ignored\|not recognized\|not supported\|not compatible\|unrecognized\|unknown" conftest.err
        if test "$?" = "0" ; then
