subdir_success="should_build=1"
subdir_failure="should_build=0"

#
# Sanity checks
#

if test "$subdir_dir" != ":" && test -d $srcdir/$subdir_dir; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: PRTE configuring in $subdir_dir" >&5
