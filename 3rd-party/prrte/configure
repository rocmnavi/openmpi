# Some helper script functions.  Unfortunately, we cannot use  kinds
# of arugments here because of the m4 substitution.  So we have to set
# special variable names before invoking the function.  :-\

prte_show_title() {
  cat <<EOF

============================================================================
== ${1}
============================================================================
EOF
  if test -n "1"
then :
  printf "%s\n" "configure:3986: === ${1}" >&5
else $as_nop
  printf "%s\n" === ${1} >&5
fi
}


prte_show_subtitle() {
  cat <<EOF

*** ${1}
EOF
  if test -n "1"
then :
  printf "%s\n" "configure:4000: *** ${1}" >&5
else $as_nop
  printf "%s\n" *** ${1} >&5
fi
}


prte_show_subsubtitle() {
  cat <<EOF

+++ ${1}
EOF
  if test -n "1"
then :
  printf "%s\n" "configure:4014: +++ ${1}" >&5
else $as_nop
  printf "%s\n" +++ ${1} >&5
fi
}

prte_show_subsubsubtitle() {
  cat <<EOF

--- ${1}
EOF
  if test -n "1"
then :
  printf "%s\n" "configure:4027: --- ${1}" >&5
else $as_nop
  printf "%s\n" --- ${1} >&5
fi
}

prte_show_verbose() {
  if test "$V" = "1"; then
      cat <<EOF
+++ VERBOSE: ${1}
EOF
      if test -n "1"
then :
  printf "%s\n" "configure:4040: --- ${1}" >&5
else $as_nop
  printf "%s\n" --- ${1} >&5
fi
  fi
}

