        as_fn_error $? "Cannot continue" "$LINENO" 5
    fi

            fi
        fi
    done

fi

    MCA_prte_iof_ALL_COMPONENTS="$all_components"
    MCA_prte_iof_STATIC_COMPONENTS="$static_components"
