                                 MCA_prte_FRAMEWORKS="$MCA_prte_FRAMEWORKS odls"
                                 MCA_prte_FRAMEWORKS_SUBDIRS="$MCA_prte_FRAMEWORKS_SUBDIRS mca/odls"
