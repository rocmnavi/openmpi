    PRTE_DYN_LIB_SUFFIX=dll
    ;;
mingw* | pw32* | cegcc*)
    PRTE_DYN_LIB_SUFFIX=dll
    ;;
darwin* | rhapsody*)
    PRTE_DYN_LIB_SUFFIX=dylib
    ;;
hpux9* | hpux10* | hpux11*)
    case $host_cpu in
        ia64*)
        PRTE_DYN_LIB_SUFFIX=so
        ;;
    *)
        PRTE_DYN_LIB_SUFFIX=sl
        ;;
    esac
    ;;
*)
   PRTE_DYN_LIB_SUFFIX=so
   ;;
esac




